s=\"form-check-input\"\n             type=\"checkbox\"\n             :disabled=\"disabled\"\n             v-model=\"model\"/>\n      <span class=\"form-check-sign\"></span>\n      <slot>\n        <span v-if=\"inline\">&nbsp;</span>\n      </slot>\n    </label>\n  </div>\n</template>\n<script>\n  export default {\n    name: 'p-checkbox',\n    model: {\n      prop: 'checked'\n    },\n    props: {\n      checked: {\n        type: [Array, Boolean],\n        description: 'Whether checkbox is checked'\n      },\n      disabled: {\n        type: Boolean,\n        description: 'Whether checkbox is disabled'\n      },\n      inline: {\n        type: Boolean,\n        description: 'Whether checkbox is inline'\n      }\n    },\n    data() {\n      return {\n        cbId: '',\n        touched: false\n      }\n    },\n    computed: {\n      model: {\n        get() {\n          return this.checked\n        },\n        set(check) {\n          if (!this.touched) {\n            this.touched = true\n          }\n          this.$emit('input', check)\n        }\n      },\n      inlineClass() {\n        if (this.inline) {\n          return `form-check-inline`\n        }\n      }\n    },\n    created() {\n      this.cbId = Math.random().toString(16).slice(2)\n    }\n  }\n</script>\n"]}]}