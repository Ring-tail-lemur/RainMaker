ue\n  },\n  toggleMinimize () {\n    document.body.classList.toggle('sidebar-mini')\n    // we simulate the window Resize so the charts will get updated in realtime.\n    const simulateWindowResize = setInterval(() => {\n      window.dispatchEvent(new Event('resize'))\n    }, 180)\n\n    // we stop the simulation of Window Resize after the animations are completed\n    setTimeout(() => {\n      clearInterval(simulateWindowResize)\n    }, 1000)\n\n    this.isMinimized = !this.isMinimized\n  }\n}\n\nconst SidebarPlugin = {\n\n  install (Vue, options) {\n    if (options && options.sidebarLinks) {\n      SidebarStore.sidebarLinks = options.sidebarLinks\n    }\n    Vue.mixin({\n      data () {\n        return {\n          sidebarStore: SidebarStore\n        }\n      }\n    })\n\n    Vue.prototype.$sidebar = SidebarStore\n    Object.defineProperty(Vue.prototype, '$sidebar', {\n      get () {\n        return this.$root.sidebarStore\n      }\n    })\n    Vue.component('side-bar', Sidebar)\n    Vue.component('sidebar-item', SidebarItem)\n  }\n}\n\nexport default SidebarPlugin\n"],"mappings":"AAAA,OAAOA,OAAP,MAAoB,eAApB;AACA,OAAOC,WAAP,MAAwB,mBAAxB;AAEA,IAAMC,YAAY,GAAG;EACnBC,WAAW,EAAE,KADM;EAEnBC,YAAY,EAAE,EAFK;EAGnBC,WAAW,EAAE,KAHM;EAInBC,cAJmB,0BAIHC,KAJG,EAII;IACrB,KAAKJ,WAAL,GAAmBI,KAAnB;EACD,CANkB;EAOnBC,cAPmB,4BAOD;IAChBC,QAAQ,CAACC,IAAT,CAAcC,SAAd,CAAwBC,MAAxB,CAA+B,cAA/B;IAEA,IAAMC,oBAAoB,GAAGC,WAAW,CAAC,YAAM;MAC7CC,MAAM,CAACC,aAAP,CAAqB,IAAIC,KAAJ,CAAU,QAAV,CAArB;IACD,CAFuC,EAErC,GAFqC,CAAxC;IAKAC,UAAU,CAAC,YAAM;MACfC,aAAa,CAACN,oBAAD,CAAb;IACD,CAFS,EAEP,IAFO,CAAV;IAIA,KAAKR,WAAL,GAAmB,CAAC,KAAKA,WAAzB;EACD;AApBkB,CAArB;AAuBA,IAAMe,aAAa,GAAG;EAEpBC,OAFoB,mBAEXC,GAFW,EAENC,OAFM,EAEG;IACrB,IAAIA,OAAO,IAAIA,OAAO,CAACnB,YAAvB,EAAqC;MACnCF,YAAY,CAACE,YAAb,GAA4BmB,OAAO,CAACnB,YAApC;IACD;;IACDkB,GAAG,CAACE,KAAJ,CAAU;MACRC,IADQ,kBACA;QACN,OAAO;UACLC,YAAY,EAAExB;QADT,CAAP;MAGD;IALO,CAAV;IAQAoB,GAAG,CAACK,SAAJ,CAAcC,QAAd,GAAyB1B,YAAzB;IACA2B,MAAM,CAACC,cAAP,CAAsBR,GAAG,CAACK,